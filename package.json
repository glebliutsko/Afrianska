{
  "name": "mediasoft-prctice",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "postcss --verbose src/css/index.css --no-map -u postcss-import -u postcss-simple-vars -u postcss-media-minmax -u postcss-csso --output src/css/index.min.css",
    "startdev": "npm run build -- --watch",
    "brsync": "npx browser-sync start --server src --no-ui --cwd 'src' --files 'index.html,css/index.min.css'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glebliutsko/MediaSoftPractice.git"
  },
