  "bugs": {
    "url": "https://github.com/glebliutsko/MediaSoftPractice/issues"
  },
  "homepage": "https://github.com/glebliutsko/MediaSoftPractice#readme",
  "dependencies": {
    "postcss-cli": "^9.1.0",
    "postcss-csso": "^6.0.0",
    "postcss-import": "^14.1.0",
    "postcss-media-minmax": "^5.0.0",
    "postcss-simple-vars": "^6.0.3"
  }
}
